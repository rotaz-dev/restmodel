{
  "name": "rotaz/restmodel",
  "description": "Eloquent's API driver.",
  "license": "MIT",
  "authors": [
    {
      "name": "Rodrigo Souza",
      "email": "rotaz-dev@rotaz.app.br"
    }
  ],
  "require": {
    "php": "^7.1.3|^8.0|^8.2|^8.4|^9.0",
    "ext-pdo_sqlite": "*",
    "ext-sqlite3": "*",
    "illuminate/database": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
    "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
  },
  "require-dev": {
    "doctrine/dbal": "^2.9 || ^3.1.4",
    "orchestra/testbench": "3.8.* || 3.9.* || ^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
    "phpunit/phpunit": "^7.5 || ^8.4 || ^9.0 || ^10.0 || ^11.0"
  },
  "autoload": {
    "psr-4": {
      "Rotaz\\RestModel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}